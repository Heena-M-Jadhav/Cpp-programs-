// for n=4
...1
..23
.345
4567
this is the output//


#include <iostream>
using namespace std;

int main()
{
 int n;
 cin>>n;
 for(int i=1; i<=n; i++){
     for(int j=1; j<=n-i; j++){
         cout<<".";
       
     }  int k,m;
        k=i; m=i;
     while(k){
         cout<<m;
         m++;
         k--;
     }
     
     cout<<endl;
 }
 
}


